<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE MartConfiguration SYSTEM "test.dtd" >

<MartConfiguration internalName="test_file">

  <Dataset  internalName="test_dataset">
 
       <StarBase>test_starbase</StarBase>
       
       <PrimaryKey>test_primaryKey</PrimaryKey>
       
       <FilterPage internalName="testFilterPage">

	       <FilterGroup internalName="testFilterGroup">

		     <FilterCollection internalName="testFilterCollection" type="list">

		       <UIFilterDescription internalName="testUIFilterDescription" 
                                    displayName="A TEST ID, DOESNT EXIST"
                                    description="For Testing Purposes Only"
                                    type="list" 
                                    fieldName="test_id"
                                    qualifier="in" 
                                    tableConstraint="gene_main"/>

		     </FilterCollection>
	
	       </FilterGroup>

       </FilterPage>
       
       <AttributePage internalName="testAttributePage">

	        <AttributeGroup internalName="testAttributeGroup">

		      <AttributeCollection internalName="testAttributeCollection" 
                                   maxSelect="1">

		         <UIAttributeDescription internalName="testUIAttributeDescription"
                                         displayName="Test of a UIAttributeDescription" 
                                         description="For Testing Purposes Only"
                                         fieldName="test_id" 
                                         tableConstraint="gene_main" 
                                         maxLength="1"
                                         source="test source"
                                         homepageURL="http://test.org"
                                         linkoutURL="http://test.org?test"/>

		      </AttributeCollection>

            </AttributeGroup>
       	    
       	</AttributePage>

   </Dataset>

</MartConfiguration>
