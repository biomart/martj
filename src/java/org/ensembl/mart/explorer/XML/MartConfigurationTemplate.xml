                                        tableConstraint="gene_structure_dm" 
                                        maxLength="9" 
                                        &ensembl_data;/>

		        <UIAttributeDescription internalName="exon_coding_chrom_start" 
                                        displayName="Exon Coding Start (Chr bp)" 
