                                        maxLength="536" 
                                        &ensembl_data;/>

                <UIAttributeDescription internalName="translation" 
                                        displayName="Ensembl Peptide ID" 
                                        fieldName="translation_stable_id_v" 
                                        tableConstraint="transcript_main" 
                                        maxLength="17" 
                                        &ensembl_data;/>

                <UIAttributeDescription internalName="external_db" 
                                        displayName="External Gene DB" 
                                        fieldName="db_name" 
                                        tableConstraint="gene_main" 
                                        maxLength="9" 
                                        &ensembl_data;/>

                <UIAttributeDescription internalName="cdna_length" 
                                        displayName="Ensembl cDNA Length" 
                                        fieldName="cdna_length" 
                                        tableConstraint="transcript_main" 
                                        maxLength="6" 
                                        &ensembl_data;/>

		      </AttributeCollection>

            </AttributeGroup>

	        <AttributeGroup internalName="exon" 
                            displayName="EXON:" 
                            description="Data on individual Exons of Genes">

		      <AttributeCollection internalName="exonatts" displayName="Exon Attributes:" maxSelect="all">

		        <UIAttributeDescription internalName="exon_name" 
                                        displayName="Ensembl Exon ID" 
                                        fieldName="exon_stable_id_v" 
                                        tableConstraint="gene_structure_dm" 
                                        maxLength="17" 
                                        &ensembl_data;/>

		        <UIAttributeDescription internalName="exon_chrom_start" 
                                        displayName="Exon Start (Chr bp)" 
                                        description="Chromosomal Coordinate of start of exon sequence." 
                                        fieldName="exon_chrom_start" 
                                        tableConstraint="gene_structure_dm" 
                                        maxLength="9" 
                                        &ensembl_data;/>

		        <UIAttributeDescription internalName="exon_chrom_end" 
                                        displayName="Exon End (Chr bp)" 
